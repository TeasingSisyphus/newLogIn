  <body>
    <div ng-app='app'>
    <div ng-controller='homepageController as homepage' >
        <nav class="dropdown">
        <ul class="nav nav-tabs">
          <li role="presentation" class="active"  ng-click='homepage.goToTab(0)'><a class='navTab'>Home</a>
          </li>
          <li role="presentation" class="dropdown">
            <a href="rules.html" class="dropdown-toggle navTab" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
            Rules<span class="caret"></span>
            </a>

            <ul class="dropdown-menu">
              <li ng-click='homepage.goToRules(1)'>Ace</li>
              <li ng-click='homepage.goToRules(2)'>Two</li>
            </ul>

          </li>
          <li role="presentation" class="active navHover"  ng-click='homepage.goToTab(2)'><a role='button' aria-haspopup='true' aria-expanded='false' class='navTab'>Cuttle</a></li>
          <li role="presentation" class="active navHover"  ng-click='homepage.goToTab(3)'><a role='button' aria-haspopup='true' aria-expanded='false' class='navTab'>About Us</a></li>
        </ul>
      </nav>
    <div id='homepageWrapper' ng-show='homepage.tabIndex===0' ng-cloak>
        <div class="row">
          <div class="col-xs-3"></div>
          <div class="col-xs-6">
            <h3 id="hoverthing">Welcome to Iceborn Gaming <small>Engineered by Ryan, powered by Ryan, supported by Sam and David</small></h3>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-3"></div>
          <div class="col-xs-6">
            Feel free to login if you already have an account, if not make an account, I don't really care, I haven't coded the login and sign up to actually work yet.
            <br>
            <br>
          </div>
        </div>
            <div class="row">
              <div class="col-xs-3 col-xs-offset-3">
                <div class="input-group">
                  <span class="input-group-btn">
                    <button class="btn btn-group-lg" type="button"><a href="cuttle.html">Sign-Up</button>
                  </span>
                  <input type="text" class="form-control" placeholder="Username">
                  <input type="text" class="form-control" placeholder="Password">
                </div>
              </div>
              <div class="col-xs-3">
                <div class="input-group">
                  <span class="input-group-btn">
                    <button class="btn btn-group-lg" type="button">Login</button>
                  </span>
                  <input type="text" class="form-control" placeholder="Username">
                  <input type="text" class="form-control" placeholder="Password">
                </div>
              </div>
            </div>
        </div>         <!-- End of homepage tab content -->
        <div id='cuttleWrapper' ng-show='homepage.tabIndex===2'>

            <form class="form-inline">
              <div class="form-group">
                <label for="gameName">Game Name</label>
                <input type="text" class="form-control" id="gameName" placeholder="Test game">
              </div>
              <div class="form-group">
                <label for="password">Password</label>
                <input type="text" class="form-control" id="password" placeholder="Optional">
              </div>
              <button type="submit" class="btn btn-default">Submit</button>
            </form>
            <table class="table table-striped">
              <th>#</th>
              <th>Game Name</th>
              <th>Status</th>
              <th>Join Game</th>
                <tr>
                  <td>1</td>
                  <td>Engarde</td>
                  <td>2/2</td>
                  <td><button>Full</button></td>
                </tr>
                <tr>
                  <td>2</td>
                  <td>You</td>
                  <td>2/2</td>
                  <td><button>Full</button></td>
                </tr>
                <tr>
                  <td>3</td>
                  <td>Sillynanny</td>
                  <td>1/2</td>
                  <td><button>Join</button></td>
                </tr>
            </table>
        </div>

</div> <!-- End of homepage controller -->
</div> <!-- End of angular module -->

